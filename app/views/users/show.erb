  <body>
    <header>
  <div class="collapse bg-dark" id="navbarHeader">
    <div class="container">
      <div class="row">
        <div class="col-sm-8 col-md-7 py-4">
          <h4 class="text-white">About</h4>
          <p class="text-muted">asdfadf</p>
        </div>
        <div class="col-sm-4 offset-md-1 py-4">
          <h4 class="text-white">Contact</h4>
          <ul class="list-unstyled">
            <li><a href="#" class="text-white">Follow on Twitter</a></li>
            <li><a href="#" class="text-white">Like on Facebook</a></li>
            <li><a href="#" class="text-white">Email me</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="navbar navbar-dark bg-dark shadow-sm">
    <div class="container d-flex justify-content-between">
      <a href="#" class="navbar-brand d-flex align-items-center">
        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" aria-hidden="true" class="mr-2" viewBox="0 0 24 24" focusable="false"><path d="M23 19a2 2 0 0 1-2 2H3a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h4l2-3h6l2 3h4a2 2 0 0 1 2 2z"/><circle cx="12" cy="13" r="4"/></svg>
        <strong>Album</strong>
      </a>
      <p>
        <p>
        <a href="/books" class="btn btn-secondary my-2">Book Home Page</a>
        <%= link_to "Log out", "/users", method: "delete" %>
        <%=link_to "edit","/users/#{@user.id}/edit" %>
      
      
    </div>
  </div>
</header>

<main role="main">

  <section class="jumbotron text-center">
    <div class="container">
      <h1 class="jumbotron-heading"> Welcome    <%=@user.user_name%></h1>
    </div>
    <div class='container'>
      <% if @user.avatar.attached? %>
            <img src='<%=(url_for(@user.avatar))%>'>
    <%end%>
    </div>
  </section>

  <div class="album py-5 bg-light">
    <div class="container">

    <%if @user.books.length > 0%>

      <div class="row">
      <% @user.books.each do |book| %>  
           
        <div class="col-md-4 container jumbotron">
          <div class="card mb-3 shadow-sm ">
            <a href = <%="/books/#{book.id}"%>>
            <img class="bd-placeholder-img card-img-top" width="200" height="225" src=<%="#{book.img_url}"%> >
            </a>

            <div class="card-body">
              <p class="card-text">Title: <%= book.title %></p>
              <p class="card-text">Category: <%= book.genre %></p>
              <p class="card-text">Author: <%= book.author%></p>
            </div>
          </div>
        </div>
        <% end %>
      <%else%>
      <div class='container text-center'>
        <h1><%= 'You dont have any books yet!'  %></h1>
      </div>
      <%end%>
      </div>
    </div>
  </div>
</main>

<footer class="text-muted">
  <div class="container">
    <p class="float-right">
      <a href="#">Back to top</a>
